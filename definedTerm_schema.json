{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "definedTerm_schema.json",
  "name": "Defined Term Schema",
  "description": "required and allowed properties of DefinedTerm",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@id": {
      "name": "URI @ids",
      "description": "required: @id is a http or https URI; no bnodes refs here",
      "$ref": "http_uri_schema.json"
    },
    "@type": {
      "type": "string",
      "format": "regex",
      "pattern": "DefinedTerm"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "termCode": {
      "type": "string"
    },
    "inDefinedTermSet": {
      "type": "string"
    }
  }
}
